import java.util.*;

public class Main{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        String a=s.nextLine();
        
        StringBuffer b=new StringBuffer(a);
        StringBuffer c=b.reverse();
        if(a.contentEquals(c))
        {
            System.out.println("Palindrome");
        }else{
            System.out.println("not");
        }
    }
}
