import java.util.*;
class Main{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        String[] b=a.split(" ");
        if(a==""){
            System.out.println("No values");
        }
        else{
            for(int i=0;i<b.length;i++){
                System.out.print(b[i]);
            
            if(i<b.length-1){
                System.out.print(",");
            }
            }
            
        }
    }
