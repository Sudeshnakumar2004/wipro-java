
import java.util.*;
 public class Main{
     public static void main(String[]args)
     {
         Scanner s=new Scanner(System.in);
         char a=s.next().charAt(0);
         
         
          if(a>=65&&a<=90||a>=97&&a<=122)
         {
             System.out.println("alphabhet");
         }else if(a>=32&&a<=47)
         {
             System.out.println("special character");
         }else
         {
             System.out.println("digit");
         }
         
     }
 }
